{
  "private": true,
  "name": "onebox-email-aggregator",
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:infra": "docker-compose up -d",
    "dev:backend": "npm --workspace=@app/backend run dev",
    "dev:frontend": "npm --workspace=@app/frontend run dev",
    "start": "npm run dev:infra && npm run dev:backend && npm run dev:frontend"
  }
}